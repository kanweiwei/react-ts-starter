        "fs-extra": "3.0.1",
        "graphql": "^0.12.3",
        "graphql-anywhere": "^4.1.3",
        "graphql-tag": "^2.6.1",
        "html-webpack-plugin": "2.29.0",
        "immutable": "^3.8.2",
        "jest": "20.0.4",
        "node-sass": "^4.7.2",
        "node-sass-chokidar": "^0.0.3",
        "normalizr": "^3.2.4",
        "npm-run-all": "^4.1.2",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "promise": "8.0.1",
