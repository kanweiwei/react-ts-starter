        "@types/prop-types": "^15.5.2",
        "@types/react-redux": "^5.0.14",
        "@types/react-router": "^4.0.21",
        "@types/react-router-dom": "^4.2.3",
        "antd": "^3.2.0",
        "autoprefixer": "7.1.6",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "classnames": "^2.2.5",
        "concurrently": "^3.5.1",
